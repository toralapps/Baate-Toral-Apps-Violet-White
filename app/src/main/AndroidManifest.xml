<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.toralapps.viowh.livecall">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.BLUETOOTH_CONNECT" />

    <application
        android:name="com.toralapps.viowh.livecall.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.VideoChat"
        tools:targetApi="n">
        <activity
            android:name="com.toralapps.viowh.livecall.activitys.LoadingScreen"
            android:exported="false" />
        <activity
            android:name="com.toralapps.viowh.livecall.activitys.LocationActivity"
            android:exported="false" />
        <activity
            android:name="com.toralapps.viowh.livecall.activitys.CallNowActivity"
            android:exported="false" />

        <activity
            android:name="com.toralapps.viowh.livecall.livevideocall.activity.CallLiveActivity"
            android:exported="false" />
        <activity
            android:name="com.toralapps.viowh.livecall.livevideocall.activity.ConnectLiveActivity"
            android:exported="false" />

        <activity
            android:name="com.toralapps.viowh.livecall.activitys.ChatRoomActivity"
            android:exported="false" />
        <activity
            android:name="com.toralapps.viowh.livecall.activitys.TermUseActivity"
            android:exported="false" />
        <activity
            android:name="com.toralapps.viowh.livecall.activitys.GenderActivity"
            android:exported="false" />
        <activity
            android:name="com.toralapps.viowh.livecall.activitys.AgeActivity"
            android:exported="false" />
        <activity
            android:name="com.toralapps.viowh.livecall.activitys.StartActivity"
            android:exported="false" />
        <activity
            android:name="com.toralapps.viowh.livecall.activitys.SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.toralapps.viowh.livecall.MainActivity"
            android:exported="true" />
        <activity
            android:name="com.toralapps.viowh.livecall.blockuser.BlockListActivity"
            android:exported="true" />


    </application>

</manifest>